@model IEnumerable<DataBase.Models.usuario>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.usu_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.usu_nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.usu_apellido)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.usu_id)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.usu_nombre)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.usu_apellido)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.usu_id }) |
        @Html.ActionLink("Details", "Details", new { id = item.usu_id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.usu_id })
    </td>
</tr>
}

</table>
